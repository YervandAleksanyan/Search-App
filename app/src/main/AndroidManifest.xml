<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.searchapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.test.searchapp.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidDevTEst">
        <activity
            android:name="com.test.searchapp.feature_search.presentation.view.SearchActivity"
            android:exported="true" />
        <activity
            android:name="com.test.searchapp.SplashActivity"
            android:exported="true"
            android:theme="@style/LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>